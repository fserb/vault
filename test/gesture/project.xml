<?xml version="1.0" encoding="utf-8"?>
<project>

	<meta title="GestureTest" package="com.fserb.GestureTest" version="1.0.0" company="fserb" />
	<app main="GestureTest" path="bin" file="GestureTest" swf-version="11" />

	<window fps="30" background="0xFAFAFA" />
	<window width="1024" height="768" resizable="false" unless="ios" />
	<window fullscreen="true" if="mobile" />
	<window orientation="landscape" vsync="true" antialiasing="0" if="ios" />
	<window require-shaders="true" />

	<source path="src" />
	<source path="../../" />

	<haxelib name="openfl" />
	<haxelib name="openfl-native" unless="flash || html5" />

	<setenv name="NUMBER_OF_PROCESSORS" value="9" />

</project>
